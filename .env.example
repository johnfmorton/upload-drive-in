APP_NAME="Upload Drive-In"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

COMPANY_NAME="Your Company Name"

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database settings
DB_CONNECTION=sqlite
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database

REDIS_CLIENT=predis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=127.0.0.1
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
VITE_DEV_SERVER_URL="http://localhost:3001"

GOOGLE_DRIVE_CLIENT_ID=
GOOGLE_DRIVE_CLIENT_SECRET=

APP_BRANDING_COLOR="#1b4cc3"

MICROSOFT_TEAMS_CLIENT_ID=
MICROSOFT_TEAMS_CLIENT_SECRET=
MICROSOFT_TEAMS_ROOT_FOLDER_ID=
DROPBOX_CLIENT_ID=
DROPBOX_CLIENT_SECRET=
DROPBOX_ROOT_FOLDER="/UploadDriveIn"

CLOUD_STORAGE_DEFAULT=google-drive

# File Manager Configuration
# Controls the number of files displayed per page in the file manager interface.
# Valid range: 1-100 (values outside this range will fall back to default)
# Default: 10 items per page if not specified
FILE_MANAGER_ITEMS_PER_PAGE=10

# Controls whether the application setup process is available.
# Set to "true" only during initial installation or reconfiguration.
# After setup is complete, set this to "false" to prevent unauthorized access.
APP_SETUP_ENABLED=true

# Queue Worker Test Security Configuration
# Maximum number of queue worker test attempts per time window
SETUP_QUEUE_TEST_MAX_ATTEMPTS=5

# Time window in minutes for rate limiting queue worker tests
SETUP_QUEUE_TEST_DECAY_MINUTES=15

# Cooldown period in seconds between queue worker tests
SETUP_QUEUE_TEST_COOLDOWN_SECONDS=30

# Upload Recovery System Configuration
# Time threshold in minutes after which an upload is considered stuck
UPLOAD_STUCK_THRESHOLD=30

# Time threshold in minutes for uploads that are actively processing
UPLOAD_PROCESSING_TIMEOUT=60

# Time threshold in hours for critical stuck upload alerts
UPLOAD_CRITICAL_STUCK_THRESHOLD=2

# Maximum number of retry attempts before marking as failed
UPLOAD_MAX_RETRIES=3

# Maximum number of recovery attempts before giving up
UPLOAD_MAX_RECOVERY=5

# Exponential backoff multiplier for retry delays (in minutes)
UPLOAD_RETRY_BACKOFF_MULTIPLIER=2

# Base delay in minutes for first retry attempt
UPLOAD_RETRY_BASE_DELAY=5

# Maximum delay in minutes between retry attempts
UPLOAD_RETRY_MAX_DELAY=60

# Batch size for processing multiple uploads
UPLOAD_RECOVERY_BATCH_SIZE=10

# Maximum number of uploads to process in a single recovery run
UPLOAD_MAX_PROCESSING_LIMIT=100

# Memory limit in MB for batch processing operations
UPLOAD_RECOVERY_MEMORY_LIMIT=512

# Maximum execution time in seconds for recovery operations
UPLOAD_RECOVERY_MAX_EXECUTION_TIME=300

# Interval in minutes for automated health checks
UPLOAD_HEALTH_CHECK_INTERVAL=15

# Minimum disk space in MB required for upload processing
UPLOAD_MIN_DISK_SPACE=1024

# Queue worker response timeout in seconds
UPLOAD_QUEUE_WORKER_TIMEOUT=30

# Google Drive API connectivity timeout in seconds
UPLOAD_API_CONNECTIVITY_TIMEOUT=10

# Token expiration warning threshold in days
UPLOAD_TOKEN_EXPIRATION_WARNING=7

# Alert threshold in hours for stuck uploads
UPLOAD_ALERT_THRESHOLD=1

# Failure rate threshold (0.1 = 10%) for triggering alerts
UPLOAD_FAILURE_RATE_THRESHOLD=0.1

# Time window in hours for calculating failure rates
UPLOAD_FAILURE_RATE_WINDOW=24

# Minimum number of uploads required before calculating failure rates
UPLOAD_FAILURE_RATE_MIN_UPLOADS=10

# Alert cooldown period in minutes to prevent spam
UPLOAD_ALERT_COOLDOWN=60

# Enable/disable automated alerting
UPLOAD_ALERTS_ENABLED=true

# Email addresses for administrative alerts (comma-separated)
UPLOAD_ALERT_RECIPIENTS=""

# Age in days after which failed uploads can be cleaned up
UPLOAD_CLEANUP_FAILED_DAYS=30

# Age in days after which successful uploads metadata can be archived
UPLOAD_ARCHIVE_SUCCESSFUL_DAYS=90

# Enable/disable automatic cleanup of orphaned files
UPLOAD_AUTO_CLEANUP_ORPHANED=true

# Maximum age in hours for temporary files before cleanup
UPLOAD_TEMP_FILE_MAX_AGE=24

# Enable detailed logging for recovery operations
UPLOAD_RECOVERY_DETAILED_LOGGING=true

# Log level for recovery operations (debug, info, warning, error)
UPLOAD_RECOVERY_LOG_LEVEL=info

# Enable performance metrics logging
UPLOAD_LOG_PERFORMANCE_METRICS=false

# Maximum number of error details to store per upload
UPLOAD_MAX_ERROR_DETAILS=10
